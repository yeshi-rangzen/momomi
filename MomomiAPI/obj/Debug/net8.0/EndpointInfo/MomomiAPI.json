  "components": {
    "schemas": {
      "GenderType": {
        "enum": [
          "Male",
          "Female",
          "NonBinary",
          "Other"
        ],
        "type": "string"
      },
      "HeritageType": {
        "enum": [
          "Arunachali",
          "Assamese",
          "Bhutanese",
          "Himachali",
          "Ladakhi",
          "Manipuri",
          "Mizo",
          "Naga",
          "Nepali",
          "Sikkimese",
          "Tibetan",
          "Tripuri",
          "Uttarakhandi",
          "Other"
        ],
        "type": "string"
      },
      "LoginWithOtpRequest": {
        "required": [
          "email",
          "otp"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "otp": {
            "maxLength": 6,
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RefreshTokenRequest": {
        "required": [
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "refreshToken": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RegisterWithOtpRequest": {
        "required": [
          "dateOfBirth",
          "email",
          "firstName",
          "gender",
          "interestedIn",
          "otp"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "otp": {
            "maxLength": 6,
            "minLength": 6,
            "type": "string"
          },
          "firstName": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "maxLength": 100,
            "type": "string",
            "nullable": true
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date-time"
          },
          "gender": {
            "$ref": "#/components/schemas/GenderType"
          },
          "interestedIn": {
            "$ref": "#/components/schemas/GenderType"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ReligionType": {
        "enum": [
          "Agnostic",
          "Animism",
          "Atheism",
          "Buddhism",
          "Christian",
          "DonyiPolo",
          "Hindu",
          "Islam",
          "Spiritual",
          "Other"
        ],
        "type": "string"
      },
      "ResendOtpRequest": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "SendMessageRequest": {
        "required": [
          "content",
          "conversationId"
        ],
        "type": "object",
        "properties": {
          "conversationId": {
            "type": "string",
            "format": "uuid"
          },
          "content": {
            "maxLength": 1000,
            "minLength": 1,
            "type": "string"
          },
          "messageType": {
            "maxLength": 20,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SendOtpRequest": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "TokenDebugRequest": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateProfileRequest": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string",
            "nullable": true
          },
          "bio": {
            "type": "string",
            "nullable": true
          },
          "heritage": {
            "$ref": "#/components/schemas/HeritageType"
          },
          "religion": {
            "$ref": "#/components/schemas/ReligionType"
          },
          "languagesSpoken": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "educationLevel": {
            "type": "string",
            "nullable": true
          },
          "occupation": {
            "type": "string",
            "nullable": true
          },
          "heightCm": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "latitude": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "longitude": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "maxDistanceKm": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "minAge": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "maxAge": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "preferredHeritage": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HeritageType"
            },
            "nullable": true
          },
          "preferredReligions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ReligionType"
            },
            "nullable": true
          },
          "culturalImportanceLevel": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "languagePreference": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme.",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
